{
    "name": "studocu/laravel-firebase",
    "description": "A Laravel package for the Firebase PHP Admin SDK",
    "keywords": ["laravel", "firebase", "firebase", "sdk", "api", "database", "fcm", "gcm"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Jérôme Gamez",
            "email": "jerome@gamez.name"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "studocu/firebase-php",
                "version": "dev-main",
                "source": {
                    "type": "git",
                    "url": "https://github.com/StuDocu/firebase-php",
                    "reference": "main"
                }
            }
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
        "studocu/firebase-php": "dev-main",
        "illuminate/contracts": "^9.0 || ^10.0 || ^11.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "symfony/cache": "^6.1.2 || ^7.0.3"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "laravel/pint": "^1.14",
        "phpunit/phpunit": "^9.6.17 || ^10.5.13"
    },
    "autoload": {
        "psr-4": {
            "Kreait\\Laravel\\Firebase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kreait\\Laravel\\Firebase\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kreait\\Laravel\\Firebase\\ServiceProvider"
            ],
            "aliases": {
                "Firebase": "Kreait\\Laravel\\Firebase\\Facades\\Firebase"
            }
        }
    },
    "scripts": {
        "cs": [
            "vendor/bin/pint"
        ],
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
